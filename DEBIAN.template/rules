#!/usr/bin/make -f

%:
	dh  $@ --with bash-completion --with systemd --with python2

override_dh_systemd_enable:
    echo "debug: override_dh_systemd_enable"
    dh_systemd_enable -p td-process-exporter debian/process-exporter.service

override_dh_systemd_start:
    echo "debug: override_dh_systemd_start"
    dh_systemd_start -p td-process-exporter debian/process-exporter.service
